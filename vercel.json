{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/kitchen-sync/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/kitchen-sync/favicon.ico",
      "dest": "/kitchen-sync/favicon.ico"
    },
    {
      "src": "/kitchen-sync/manifest.json",
      "dest": "/kitchen-sync/manifest.json"
    },
    {
      "src": "/kitchen-sync/(.*)",
      "dest": "/kitchen-sync/index.html"
    }
  ]
} 